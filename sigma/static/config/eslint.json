{
    "env": {
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": 0
    },
    "globals": {
        "$": true,
        "_": true,
        "_urlPrefixes": true,
        "angular": true,
        "inject": true,
        "window": true,
        "btoa": true
    }
    // "colors": true
}